(define (problem myProblem)
  (:domain myDomain)

    (:objects
    lamp1 - Thing
    input5 - integer 
    input13 - integer 
    )

  (:init
    (not (power lamp1))
    (not (powerRead lamp1))
    (= (invokeActionIncreaseBrightnessInput input5 lamp1) 5)
    (= (writePropertyBrightnessInput input13 lamp1) 5)
    (= (brightness lamp1) 0)

  )

  (:goal
    (= (brightness lamp1) 5)
  )
)